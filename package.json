{
  "name": "autojsdoccomments",
  "publisher": "2Lynk",
  "displayName": "AutoJSDocComments",
  "description": "Automatically add JSDoc comments to your code ",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/2Lynk/autojsdoccomments.git"
  },
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autojsdoccomments.generateComment",
        "title": "AutoJSDocComments > Generate JSDoc comment"
      },
      {
        "command": "autojsdoccomments.deleteApiKey",
        "title": "AutoJSDocComments > Delete saved API key"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "openai": "^4.20.1"
  }
}
